{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "resolvePackageJsonExports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2022",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/arbitrage-packager": ["libs/arbitrage-packager/src"],
      "@app/arbitrage-packager/*": ["libs/arbitrage-packager/src/*"],
      "@shared/data-import": ["libs/data-import/src"],
      "@shared/data-import/*": ["libs/data-import/src/*"],
      "@eve/shared": ["../../packages/shared/src/index.ts"],
      "@eve/shared/*": ["../../packages/shared/src/*"],
      "@eve/prisma": ["../../packages/prisma/client/index.d.ts"],
      "@eve/prisma/*": ["../../packages/prisma/client/*"],
      "@eve/api-client": ["../../packages/api-client/src/index.ts"],
      "@eve/api-client/*": ["../../packages/api-client/src/*"],
      "@eve/eve-core": ["../../packages/eve-core/src/index.ts"],
      "@eve/eve-core/*": ["../../packages/eve-core/src/*"],
      "@eve/eve-esi": ["../../packages/eve-esi/src/index.ts"],
      "@eve/eve-esi/*": ["../../packages/eve-esi/src/*"],
      "@eve/api-contracts": ["../../packages/api-contracts/src/index.ts"],
      "@eve/api-contracts/*": ["../../packages/api-contracts/src/*"],
      "@api/common/*": ["src/common/*"],
      "@api/characters/*": ["src/characters/*"],
      "@api/notifications/*": ["src/notifications/*"],
      "@api/esi/*": ["src/esi/*"],
      "@api/game-data/*": ["src/game-data/*"],
      "@api/prisma/*": ["src/prisma/*"],
      "@api/support/*": ["src/support/*"],
      "@api/tradecraft/*": ["src/tradecraft/*"],
      "@api/skill-plans/*": ["src/skill-plans/*"],
      "@api/skill-farm/*": ["src/skill-farm/*"]
    }
  }
}
