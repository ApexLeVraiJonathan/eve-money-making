{
  "name": "@eve-money-making/api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "pnpm --filter @eve/prisma prisma:generate",
    "db:push": "pnpm --filter @eve/prisma run prisma db push",
    "db:migrate": "pnpm --filter @eve/prisma run prisma migrate dev",
    "prisma:migrate:deploy": "pnpm --filter @eve/prisma prisma:migrate:deploy",
    "db:studio": "pnpm --filter @eve/prisma prisma:studio",
    "db:seed": "pnpm --filter @eve/prisma run prisma db seed",
    "db:reset": "pnpm --filter @eve/prisma run prisma migrate reset",
    "db:reset-prod": "tsx scripts/reset-prod-db.ts",
    "fix:allocation": "tsx scripts/fix-allocation.ts",
    "fix:missing-table": "tsx scripts/fix-missing-table.ts",
    "audit:endpoints": "tsx scripts/audit-endpoints.ts",
    "check:env": "tsx scripts/check-env.ts",
    "backfill:listed-units": "tsx scripts/reset-and-backfill-listed-units.ts"
  },
  "dependencies": {
    "@eve/eve-core": "workspace:*",
    "@eve/eve-esi": "workspace:*",
    "@eve/prisma": "workspace:*",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/adapter-pg": "^7.1.0",
    "axios": "^1.12.2",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "unzipper": "^0.11.4",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/body-parser": "^1.19.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/test/setup-jest.ts"
    ],
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/",
      "<rootDir>/test/"
    ],
    "moduleNameMapper": {
      "^@app/arbitrage-packager(|/.*)$": "<rootDir>/libs/arbitrage-packager/src/$1",
      "^@app/lib(|/.*)$": "<rootDir>/libs/lib/src/$1",
      "^@shared/data-import(|/.*)$": "<rootDir>/libs/data-import/src/$1",
      "^@api/common/(.*)$": "<rootDir>/src/common/$1",
      "^@api/characters/(.*)$": "<rootDir>/src/characters/$1",
      "^@api/esi/(.*)$": "<rootDir>/src/esi/$1",
      "^@api/game-data/(.*)$": "<rootDir>/src/game-data/$1",
      "^@api/prisma/(.*)$": "<rootDir>/src/prisma/$1",
      "^@api/support/(.*)$": "<rootDir>/src/support/$1",
      "^@api/tradecraft/(.*)$": "<rootDir>/src/tradecraft/$1"
    }
  }
}
