{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "arbitrage-packager": {
      "type": "library",
      "root": "libs/arbitrage-packager",
      "entryFile": "index",
      "sourceRoot": "libs/arbitrage-packager/src",
      "compilerOptions": {
        "tsConfigPath": "libs/arbitrage-packager/tsconfig.lib.json"
      }
    },
    "data-import": {
      "type": "library",
      "root": "libs/data-import",
      "entryFile": "index",
      "sourceRoot": "libs/data-import/src",
      "compilerOptions": {
        "tsConfigPath": "libs/data-import/tsconfig.lib.json"
      }
    }
  }
}