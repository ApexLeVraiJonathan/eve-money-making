{
  "name": "eve-money-making",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "build:api": "pnpm --filter @eve-money-making/api run build",
    "build:web": "pnpm --filter @eve-money-making/web run build",
    "dev": "pnpm --filter api dev",
    "dev:api": "pnpm --filter @eve-money-making/api run start:dev",
    "dev:web": "pnpm --filter @eve-money-making/web run dev",
    "format": "pnpm -r run format",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint --fix",
    "type-check": "pnpm -r exec tsc --noEmit",
    "test": "pnpm -r run test",
    "test:api": "pnpm --filter @eve-money-making/api run test",
    "test:web": "pnpm --filter @eve-money-making/web run test",
    "install:all": "pnpm install",
    "clean": "pnpm -r exec rm -rf dist coverage .next node_modules/.cache",
    "db:up": "docker compose -f docker-compose.dev.yml up -d",
    "db:down": "docker compose -f docker-compose.dev.yml down",
    "db:reset": "docker compose -f docker-compose.dev.yml down -v && docker compose -f docker-compose.dev.yml up -d",
    "db:logs": "docker compose -f docker-compose.dev.yml logs -f postgres",
    "db:generate": "pnpm --filter @eve/prisma prisma:generate",
    "db:migrate:dev": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5433/eve_money_dev?schema=public pnpm --filter @eve/prisma prisma:migrate:deploy",
    "db:migrate:deploy": "pnpm --filter @eve/prisma prisma:migrate:deploy",
    "db:studio": "pnpm --filter @eve/prisma prisma:studio",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0"
}
